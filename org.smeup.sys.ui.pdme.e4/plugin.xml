<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   
   

   <extension point="org.eclipse.ui.popupMenus">
         
      <objectContribution
            id="org.asup.ui.pdme.e4.ecore.contribution1"
            nameFilter="*.ecore"
            objectClass="org.eclipse.core.resources.IFile">
         <menu
               icon="icons/asup_24.png"
               id="org.asup.ui.pdme.e4.ecore.menu1"
               label="As.UP Model Management"
               path="additions">
            <separator name="generator"/>
         </menu>
         <action
               label="Create genmodel file"
               class="org.smeup.sys.ui.pdme.e4.actions.GenModelGeneratorAction"
               menubarPath="org.asup.ui.pdme.e4.ecore.menu1/generator"
               enablesFor="1"
               id="org.asup.ui.pdme.e4.ecore.genaction">
         </action>
      </objectContribution>
      <objectContribution
            id="org.asup.ui.pdme.e4.ecore.contribution2"
            nameFilter="commands.xmi"
            objectClass="org.eclipse.core.resources.IFile">
         <menu
               icon="icons/asup_24.png"
               id="org.asup.ui.pdme.e4.ecore.menu2"
               label="As.UP Commands Management"
               path="additions">
            <separator
                  name="generator">
            </separator>
         </menu>
         <action
               class="org.smeup.sys.ui.pdme.e4.actions.NormalizeCommandsAction"
               enablesFor="1"
               id="org.asup.ui.pdme.e4.ecore.normalizeCommands"
               label="Create genmodel file"
               menubarPath="org.asup.ui.pdme.e4.ecore.menu2/generator">
         </action>
      </objectContribution>
      

      
      
         
   </extension>

</plugin>
