<?xml version="1.0" encoding="UTF-8"?>
<os-cmd:CommandContainer
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:il-core="http://www.smeup.org/asup/il/core"
    xmlns:il-data-def="http://www.smeup.org/asup/il/data/def"
    xmlns:os-cmd="http://www.smeup.org/asup/os/cmd"
    xsi:schemaLocation="http://www.smeup.org/asup/il/core ../../org.smeup.sys.il.core/model/il-core.ecore http://www.smeup.org/asup/il/data/def ../../org.smeup.sys.il.data/model/il-data.ecore#//def http://www.smeup.org/asup/os/cmd ../../org.smeup.sys.os.cmd/model/os-cmd.ecore">
  <contents
      xsi:type="os-cmd:Command"
      name="RUNTEST"
      text="Run test program"
      program="QRUNTEST">
    <parameters
        name="COMP"
        position="1">
      <dataTerm
          name="component"
          text="">
        <definition
            xsi:type="il-data-def:CharacterDef"
            length="32"
            varying="true"/>
      </dataTerm>
    </parameters>
    <parameters
        name="OUTPUT"
        position="4">
      <dataTerm
          name="output"
          restricted="true">
        <facets
            xsi:type="il-core:Special">
          <elements
              name="*"
              value="*"/>
          <elements
              name="*FILE"
              value="F"/>
          <elements
              name="*PRINT"
              value="P"/>
        </facets>
        <default
            value="*"/>
        <definition
            xsi:type="il-data-def:CharacterDef"
            length="1"/>
      </dataTerm>
    </parameters>
    <parameters
        name="MOD"
        position="2">
      <dataTerm
          name="module"
          text="">
        <definition
            xsi:type="il-data-def:CharacterDef"
            length="32"
            varying="true"/>
      </dataTerm>
    </parameters>
    <parameters
        name="OBJ"
        position="3">
      <dataTerm
          name="object"
          text="">
        <definition
            xsi:type="il-data-def:CharacterDef"
            length="32"
            varying="true"/>
      </dataTerm>
    </parameters>
  </contents>
</os-cmd:CommandContainer>
