<?xml version="1.0" encoding="ASCII"?>
<il-flow:Module
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:il-data="http://www.smeup.org/asup/il/data"
    xmlns:il-flow="http://www.smeup.org/asup/il/flow"
    xsi:schemaLocation="http://www.smeup.org/asup/il/data ../../org.smeup.sys.il.data/model/il-data.ecore http://www.smeup.org/asup/il/flow ../../org.smeup.sys.il.flow/model/il-flow.ecore"
    name="*SQL">
  <dataSection>
    <datas xsi:type="il-data:UnaryCompoundDataTerm"
        name="SQLCA">
      <definition
          xsi:type="il-data:DataStructDef">
        <elements
            xsi:type="il-data:UnaryAtomicDataTerm"
            name="SQLAID">
          <definition
              xsi:type="il-data:CharacterDef"
              length="8"/>
        </elements>
        <elements
            xsi:type="il-data:UnaryAtomicDataTerm"
            name="SQLCABC">
          <definition
              xsi:type="il-data:BinaryDef"
              type="INTEGER"/>
        </elements>
        <elements
            xsi:type="il-data:UnaryAtomicDataTerm"
            name="SQLCOD">
          <definition
              xsi:type="il-data:BinaryDef"
              type="INTEGER"/>
        </elements>
      </definition>
    </datas>
  </dataSection>
  <flowSection>
    <prototypes>
      <delegate
          xsi:type="il-data:UnaryAtomicDataTerm"
          name="%execute"/>
      <entry>
        <parameters>
          <delegate
              xsi:type="il-data:UnaryAtomicDataTerm"
              name="statement"
              default=""
              mandatory="true">
            <definition
                xsi:type="il-data:StringDef"/>
          </delegate>
        </parameters>
      </entry>
    </prototypes>
    <prototypes>
      <delegate
          xsi:type="il-data:UnaryAtomicDataTerm"
          name="%execute"/>
      <entry>
        <parameters>
          <delegate
              xsi:type="il-data:UnaryAtomicDataTerm"
              name="query"
              default=""
              mandatory="true">
            <definition
                xsi:type="il-data:StringDef"/>
          </delegate>
        </parameters>
        <parameters>
          <delegate
              xsi:type="il-data:UnaryAtomicDataTerm"
              name="into"
              default=""
              mandatory="true">
            <definition
                xsi:type="il-data:BufferDef"/>
          </delegate>
        </parameters>
      </entry>
    </prototypes>
  </flowSection>
</il-flow:Module>
