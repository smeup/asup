  <contents
      xsi:type="os-cmd:Command"
      name="DLTMOD"
      text="Delete Module"
      program="QLIDLOBJ"
      status="POS">
    <parameters
        allowVariable="true"
        name="MODULE"
        position="1"
        status="POS">
      <dataTerm
          name="module">
        <cardinality
            min="1"/>
        <definition
            xsi:type="il-data-def:DataStructDef"
            qualified="true">
          <elements
              xsi:type="os-cmd:CommandData"
              name="nameGeneric">
            <facets
                xsi:type="il-data-term:Format"
                type="GENERIC"/>
            <cardinality
                min="1"/>
            <definition
                xsi:type="il-data-def:CharacterDef"
                length="10"/>
          </elements>
          <elements
              xsi:type="os-cmd:CommandData"
              name="library">
            <facets
                xsi:type="il-data-term:Format"
                type="NAME"/>
            <facets
                xsi:type="il-data-term:Special">
              <elements
                  name="*LIBL"
                  value="*LIBL"/>
              <elements
                  name="*CURLIB"
                  value="*CURLIB"/>
              <elements
                  name="*USRLIBL"
                  value="*USRLIBL"/>
              <elements
                  name="*ALL"
                  value="*ALL"/>
              <elements
                  name="*ALLUSR"
                  value="*ALLUSR"/>
            </facets>
            <default
                value="*LIBL"/>
